#include<stdio.h>
#include<conio.h>
#include<math.h>
#include<string.h>
main()
{
	int l; char str[100];
 
printf("\nEnter The Password\n");
gets(str);
l=strlen(str);
printf("%d",l);
if(l>18)
{
	printf("\nSTRONG PASSWORD\n");
}
else if(l>8)
{	
	printf("\nNOT SO STRONG PASSWORD\n");
}
else
	printf("\nWEAK PASSWORD\n");
getch();
}
